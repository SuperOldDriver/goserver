<Fmt>
    <!--
     用SpitString成数组，数组元素包含ItemInclude
     -->
    <Spit SpitString="repo-list-item d-flex flex-column flex-md-row flex-justify-start py-4 public source"
          ItemInclude="col-12 col-md-8 pr-md-3">
        <Param>
            <Regexp Name="Url,Name" Index="-2,-1">
                <![CDATA[data-hydro-click-hmac="[^"]*" href="([^"]*)">([^<]*)</a>]]></Regexp>
            <Regexp Name="Star" Index="-1">
                <![CDATA[</svg>\s*([^<\s]*)\s*</a>]]></Regexp>
            <Regexp Name="Summary" Index="-1">
                <![CDATA[<p class="col-12 col-md-9 d-inline-block text-gray mb-2 pr-4">\s*([^\r\n]*)\s*</p>]]>
            </Regexp>
        </Param>
        <Sql>
            <Check Param="Url">select count(*) from Project where Github=?</Check>
            <Insert Param="Url,Name,Star,Summary">INSERT INTO `Project`(`Github`,`Name`,`CreateAt`,`CatId`,`Star`,Summary)VALUES(?,?,now(),'0',?,?)
            </Insert>
            <Update Param="Star,Summary,Url">update `Project` set `Star`=?,Summary=? where `Github`=?</Update>
        </Sql>
    </Spit>
</Fmt>